{
  "name": "saladict",
  "version": "6.33.2",
  "description": "Chrome extension and Firefox WebExtension, inline translator powered by multiple online dictionaries",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "devbuild": "node scripts/build.js --devbuild",
    "zip": "node scripts/pack.js",
    "test": "node scripts/test.js --env=jsdom",
    "commit": "git-cz",
    "release": "standard-version",
    "pdf": "node scripts/pdf.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">= 8.9.4",
    "npm": ">= 5.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crimx/ext-saladict"
  },
  "homepage": "https://github.com/crimx/ext-saladict",
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/crimx/create-react-typescript-sass-webextension/issues"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "resolutions": {
    "@types/react": "*"
  },
  "dependencies": {
    "@types/dompurify": "^0.0.31",
    "@types/i18next": "^8.4.3",
    "@types/lodash": "^4.14.98",
